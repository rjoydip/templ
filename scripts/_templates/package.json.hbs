{
  "name": "@templ/{{ name }}",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "TEMPL {{ name }} service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/{{ name }}#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "packages/{{ name }}"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo dist docs coverage",
    "dev": "tsx ./src/index.ts --watch",
    "docs:build": "pnpm typedoc",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "size:limit": "size-limit",
    "test": "vitest run",
    "test:ci": "CI=true vitest run",
    "test:watch": "vitest run --watch",
    "typedoc": "typedoc src/index.ts",
    "typedoc:preview": "sirv docs"
  },
  "devDependencies": {
    "@size-limit/file": "^8.2.6",
    "@templ/config": "workspace:^",
    "@templ/eslint-config": "workspace:^",
    "@templ/tsconfig": "workspace:^",
    "@types/node": "^20.5.7",
    "@vitest/coverage-v8": "^0.34.3",
    "eslint": "^8.48.0",
    "rimraf": "^5.0.1",
    "sirv-cli": "^2.0.2",
    "size-limit": "^8.2.6",
    "tsx": "^3.12.7",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  },
  "size-limit": []
}

{{#expect name}}
