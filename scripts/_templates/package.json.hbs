{{#expect name}}
{
  "name": "@templ/{{ name }}",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "TEMPL {{ name }} service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/{{ name }}#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "packages/{{ name }}"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo dist docs coverage",
    "docs:preview": "sirv docs",
    "docs:build": "typedoc src/index.ts",
    "dev": "tsup ./src/index.ts --watch",
    "test": "vitest run",
    "test:ci": "CI=true vitest run",
    "test:watch": "vitest run --watch"
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "sirv-cli": "^2.0.2",
    "typedoc": "^0.24.8",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  }
}
