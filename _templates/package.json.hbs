{{#expect name}}
{
  "name": "@gfft/{{ name }}",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "GFFT {{ name }} service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/gfft/tree/main/packages/{{ name }}#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/gfft.git",
    "directory": "packages/{{ name }}"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/gfft/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup ./src/index.ts --watch"
  },
  "devDependencies": {
    "esno": "^0.16.3",
    "tsup": "^7.1.0"
  },
  "dependencies": {}
}
