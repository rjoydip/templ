{{#expect name}}
{
  "name": "@templ/{{ name }}",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "TEMPL {{ name }} service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/{{ name }}#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "packages/{{ name }}"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup ./src/index.ts --watch"
  },
  "devDependencies": {},
  "dependencies": {
    "@templ/config": "workspace:^"
  }
}
