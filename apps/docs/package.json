{
  "name": "@templ/docs",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "TEMPL docs",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/docs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "docs"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "scripts": {
    "clean": "wireit",
    "build": "wireit",
    "docs:dev": "wireit",
    "docs:preview": "wireit",
    "eslint": "wireit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.2",
    "eslint": "^8.50.0",
    "vitepress": "1.0.0-rc.20",
    "vue": "^3.3.4",
    "wireit": "^0.14.0"
  },
  "wireit": {
    "clean": {
      "command": "rimraf .vitepress .wireit"
    },
    "build": {
      "command": "vitepress build .",
      "output": [
        "build/**"
      ]
    },
    "docs:dev": {
      "command": "vitepress dev ."
    },
    "docs:preview": {
      "command": "vitepress preview .",
      "files": [
        "*.md"
      ],
      "output": [
        "build/**"
      ],
      "dependencies": [
        "build"
      ]
    },
    "eslint": {
      "command": "eslint --color --cache --fix --cache-location .eslintcache .",
      "files": [
        "src/**/*.ts",
        "eslint.config.js"
      ]
    }
  }
}
