{
  "name": "@templ/api",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "description": "API",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "apps/api"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0",
    "npm": ">=8"
  },
  "scripts": {
    "build": "tsx esbuild.config.ts",
    "clean": "del-cli .turbo dist coverage",
    "dev": "tsx ./src/index",
    "api:load": "tsx ./test/api.load.ts",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.5",
    "@hono/zod-openapi": "^0.15.3",
    "@scalar/hono-api-reference": "^0.5.159",
    "consola": "^3.2.3",
    "hono": "^4.6.9"
  },
  "devDependencies": {
    "@templ/config": "workspace:*",
    "@templ/tsconfig": "workspace:*",
    "@types/autocannon": "^7.12.5",
    "@types/node": "^20.17.6",
    "@vitest/coverage-v8": "^1.6.0",
    "autocannon": "^7.15.0",
    "del-cli": "^5.1.0",
    "esbuild": "^0.20.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  },
  "size-limit": "1024 B"
}
