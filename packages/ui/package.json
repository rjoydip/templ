{
  "private": true,
  "name": "@templ/ui",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:storybook": "storybook build",
    "clean": "rimraf .nyc_output .turbo dist docs storybook-static",
    "dev:storybook": "storybook dev -p 6006",
    "docs:preview": "sirv docs",
    "docs:build": "typedoc src/index.ts"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "preact": "^10.16.0"
  },
  "devDependencies": {
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-interactions": "7.2.1",
    "@storybook/addon-links": "7.2.1",
    "@storybook/addon-storysource": "^7.2.2",
    "@storybook/blocks": "7.2.1",
    "@storybook/preact": "^7.2.2",
    "@storybook/preact-vite": "^7.2.2",
    "@templ/config": "workspace:^",
    "@testing-library/preact": "^3.2.3",
    "rimraf": "^5.0.1",
    "sirv-cli": "^2.0.2",
    "storybook": "7.2.1",
    "typedoc": "^0.24.8"
  }
}
