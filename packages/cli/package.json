{
  "name": "@grft/cli",
  "type": "module",
  "version": "0.1.0",
  "cliname": "grft",
  "description": "Grft cli service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/grft/tree/main/packages/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/grft.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/grft/issues"
  },
  "sideEffects": true,
  "exports": {
    "./*": "./*"
  },
  "main": "./dist/cli.js",
  "module": "./dist/cli.js",
  "bin": {
    "grft": "bin/grft.mjs"
  },
  "files": [
    "bin",
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "_ni": "ni",
    "build": "tsup",
    "dev": "esno ./src/cli.ts -v",
    "clean:nm": "rimraf node_modules",
    "clean": "rimraf package-lock.json dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "CI=true vitest",
    "test:coverage": "nr test --coverage",
    "test:cli": "pnpm link . && npx grft -v && rm -r node_modules/{@grft,.bin/*grft*}"
  },
  "dependencies": {
    "@grft/core": "workspace:*",
    "sade": "^1.8.1",
    "serialize-error": "^11.0.0"
  },
  "devDependencies": {
    "execa": "^7.1.1"
  }
}
