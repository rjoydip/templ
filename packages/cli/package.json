{
  "name": "@templ/cli",
  "cliname": "TEMPL",
  "type": "module",
  "version": "0.0.1",
  "description": "Cli",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "sideEffects": true,
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "templjs": "templ.mjs"
  },
  "files": [
    "*.d.ts",
    "dist",
    "templ.mjs"
  ],
  "engines": {
    "node": "^18.8.0 || >=20.6.0",
    "npm": ">=8"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --tree-shaking=true --target=node20 --packages=external --format=esm --platform=node --outfile=dist/index.js",
    "dev": "esbuild src/index.ts --watch --bundle --minify --tree-shaking=true --target=node20 --packages=external --format=esm --platform=node --outfile=dist/index.js",
    "postbuild": "pnpm link . && pnpm templjs -v"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "esbuild": "^0.20.0"
  },
  "size-limit": "400 KB"
}
