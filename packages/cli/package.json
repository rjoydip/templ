{
  "name": "@grft/cli",
  "type": "module",
  "version": "0.1.0",
  "cliname": "GRFT",
  "description": "Grft cli service",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/grft/tree/main/packages/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/grft.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/grft/issues"
  },
  "sideEffects": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "grft": "grft.mjs"
  },
  "files": [
    "grft.mjs",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "esno ./src/index.ts -v",
    "test:cli": "pnpm link . && pnpm grft -v",
    "build:core": "pnpm --filter core run build"
  },
  "dependencies": {
    "@grft/core": "workspace:*",
    "sade": "^1.8.1",
    "serialize-error": "^11.0.0"
  },
  "devDependencies": {
    "execa": "^7.1.1",
    "find-monorepo-root": "^1.0.3"
  }
}
