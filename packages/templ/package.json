{
  "name": "templ",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "description": "Templ",
  "license": "MIT",
  "homepage": "https://github.com/rjoydip/templ/tree/main/packages/templ#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/templ.git",
    "directory": "packages/templ"
  },
  "bugs": {
    "url": "https://github.com/rjoydip/templ/issues"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": "^18.8.0 || >=20.6.0",
    "npm": ">=8"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --tree-shaking=true --target=node20 --packages=external --format=esm --platform=node --outfile=dist/index.js",
    "dev": "esbuild src/index.ts --watch --bundle --minify --tree-shaking=true --target=node20 --packages=external --format=esm --platform=node --outfile=dist/index.js"
  },
  "dependencies": {
    "consola": "^3.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "esbuild": "^0.20.0"
  },
  "size-limit": "100 B"
}
